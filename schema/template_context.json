  "$id": "https://raw.githubusercontent.com/NSSAC/templated_configuration_system/main/schema/template_context.json",
  "description": "Captured user input",
  "type": "object",
  "properties": {
    "templateId": {
      "type": "string",
      "format": "uri-reference"
    },
    "parameters": {
